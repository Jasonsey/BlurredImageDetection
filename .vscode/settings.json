{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/data/**": true
    },
    "python.linting.pylintArgs": [
        "--init-hook",
        "import sys; sys.path.append('src/api/cs542')",
        "--disable=C0301,C0111,R0913",
        "--generate-members"
    ]
}
